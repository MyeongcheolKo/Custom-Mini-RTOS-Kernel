#include "drivers.h"


/* pins used:
 * PB15 - SPI2_MOSI
 * PB14 - SPI2_MISO
 * PB13 - SPI2_SCK
 * PB12 - SPI2_NSS
 * GPIO alternate function mode: 5
 *
 */

void SPI_GPIO_pins_init(void)
{
	GPIO_Handle_t SPI_pins;

	SPI_pins.p_GPIOx = GPIOB;
	//ENABLE GPIO clock
	GPIO_clock_control(SPI_pins.p_GPIOx, ENABLE);

	//pin configuration generic to all SPI pins
	SPI_pins.GPIO_config.GPIO_pin_mode = GPIO_MODE_ALTFUNC;


}
int main(void)
{
	return 0;
}


